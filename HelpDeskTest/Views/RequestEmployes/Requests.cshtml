@model IEnumerable<HelpDeskTest.Models.Request>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RequestId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StatusId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RequestTypeID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateOfRegistration)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.RequestId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.EmployeId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.StatusId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.RequestTypeID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DateOfRegistration)
    </td>

    <td>
        @Html.ActionLink("Подробнее", "Details", new { id = item.RequestId }, new { @class = "btn btn-success" })
       
    </td>
</tr>
}

</table>
