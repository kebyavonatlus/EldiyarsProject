@model List<HelpDeskTest.Models.UserRolesViewModel>
@{
    ViewBag.Title = "Users";
}

<table class="table table-hover">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>@user.RoleName</td>
            <td>
                @Html.ActionLink("Изменить",
                    "EditUser",
                    "UserRoles",
                    new {userID = user.UserId},
                    new {@class="btn btn-success"})
            </td>
        </tr>
    }
</table>

